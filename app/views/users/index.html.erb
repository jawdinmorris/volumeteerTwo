<p id="notice"><%= notice %></p>
<h1> For Admin User </h1>
<h1> Users </h1>
<h2>Volunteers</h2>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @volunteers.each do |volunteer| %>
      <tr>
        <td><%= volunteer.email %> </td>
        <td><%= link_to 'Show', volunteer %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<h1>Charities</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @charities.each do |charity| %>
      <tr>
        <td><%= charity.email %> </td>
        <td><%= link_to 'Show', charity %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
